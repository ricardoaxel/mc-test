{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@app/*": ["src/app/*"],
      "@assets/*": ["src/assets/*"],
      "@atomic/*": ["src/atomic/*"],
      "@utils/*": ["src/utils/*"]
    },
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}
